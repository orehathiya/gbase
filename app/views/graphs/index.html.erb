<h1>Listing graphs</h1>

<ul class="thumbnails">
  <% @graphs.each do |graph| %>
    <li class="span3">
      <div class="thumbnail">
        <p>
        <%= graph.created_at %>
        </p>
        <p class="font12">
        <%= link_to graph.title, graph %>
        </p>
        <p class="graph_desc">
        <%= graph.description %>
        </p>
      </div>
    </li>
  <% end %>
</ul>

<%= will_paginate @graphs %>
