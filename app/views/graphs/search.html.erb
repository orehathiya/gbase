<h1>Search result</h1>

<div class="thumb_list">
  <% @graphs.each do |graph| %>
    <div class="thumb_col">
      <p>
      <%= graph.created_at %>
      </p>
      <p class="font12">
      <%= link_to graph.title, graph %>
      </p>
      <p>
      <%= graph.description %>
      </p>
      <%= link_to 'Show', graph %>
      <%= link_to 'Edit', edit_graph_path(graph) %>
      <%= link_to 'Destroy', graph, method: :delete, data: { confirm: 'Are you sure?' } %>
    </div>
  <% end %>
</div>

<%= will_paginate @graphs %>
